UPDATE AMENITIES_LIST
SET DATEOFAQUISITION = '2023-03-17', ITEMCOST = 100, ITEMCONDITION = 'Excellent'
WHERE USERID = 1  AND ADDRESSID=6 AND AMENITYID = 17;

UPDATE AMENITIES_LIST
SET DATEOFAQUISITION = '2023-10-16', ITEMCOST = 130, ITEMCONDITION = 'Good'
WHERE USERID = 2  AND ADDRESSID=4 AND AMENITYID = 32;

UPDATE AMENITIES_LIST
SET DATEOFAQUISITION = '2023-01-15', ITEMCOST = 120, ITEMCONDITION = 'Fair'
WHERE USERID = 4  AND ADDRESSID=10 AND AMENITYID = 32;

UPDATE AMENITIES_LIST
SET DATEOFAQUISITION = '2023-06-15', ITEMCOST = 220, ITEMCONDITION = 'Good'
WHERE USERID = 10  AND ADDRESSID=19 AND AMENITYID = 1;
 commit;
