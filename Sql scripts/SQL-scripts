#—---------------PROCEDURE—---------------------------------

#Create a stored procedure called CheckUser and add the following code and press apply: 
Delimiter $
CREATE DEFINER=`root`@`localhost` PROCEDURE `CheckUser`( IN _userName varchar(100), IN _password varchar(100), out _result int )
BEGIN 
		
    declare _User varchar(100);
		SELECT username into _User FROM USER_LIST WHERE `LOGINPASSWORD` = _password  AND USERNAME = _userName;
        IF (isnull(_User)) THEN  
			BEGIN
				SET _result = 0;
			END;
		ELSE
        BEGIN
				SET _result = 1;
		END;
        END IF;
    END
Delimiter ; 



