CREATE DATABASE IF NOT EXISTS airbnb_inventory;
USE airbnb_inventory;

DROP TABLE IF EXISTS USER_LIST, ADDRESS, AMENITIES_LIST, AMENITY, TRANSACTIONS;

CREATE TABLE USER_LIST(
USERID INT NOT NULL PRIMARY KEY,
USERNAME VARCHAR(100) UNIQUE,
LOGINPASSWORD VARCHAR(100),
FIRSTNAME VARCHAR(100),
LASTNAME VARCHAR(100),
EMAIL VARCHAR(255),
USERTYPE VARCHAR(50)
);

CREATE TABLE ADDRESS(
ADDRESSID INT NOT NULL PRIMARY KEY,
STREET1 VARCHAR(100),
STREET2 VARCHAR(100),
CITY VARCHAR(100),
STATE VARCHAR(50),
ZIPCODE VARCHAR(50),
LONGITUDE FLOAT (11),
LATITUDE FLOAT (11),
USERID INT,
FOREIGN KEY (USERID) REFERENCES USER_LIST(USERID));

CREATE TABLE AMENITY(
AMENITYID INT NOT NULL PRIMARY KEY,
AMENITYNAME VARCHAR(100)
);


CREATE TABLE AMENITIES_LIST(
USERID INT,
ADDRESSID INT,
AMENITYID INT,
QUANTITY INT,
DATEOFAQUISITION DATE,
ITEMCOST INT, 
ITEMCONDITION VARCHAR(100), 
PRIMARY KEY (USERID, ADDRESSID, AMENITYID),
FOREIGN KEY (AMENITYID) REFERENCES AMENITY(AMENITYID),
FOREIGN KEY (ADDRESSID) REFERENCES ADDRESS(ADDRESSID),
FOREIGN KEY (USERID) REFERENCES USER_LIST(USERID));

CREATE TABLE TRANSACTIONS(
USERID_HOST INT,
USERID_GUEST INT,
ADDRESSID INT,
AMENITYID INT,
COUNT INT,
START_DATE DATE,
END_DATE DATE,
FOREIGN KEY (USERID_HOST ) REFERENCES USER_LIST(USERID),
FOREIGN KEY (USERID_GUEST  ) REFERENCES USER_LIST(USERID),
FOREIGN KEY (ADDRESSID) REFERENCES ADDRESS(ADDRESSID),
FOREIGN KEY (AMENITYID) REFERENCES AMENITY(AMENITYID));



